{
  "name": "node-fetch-demo",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Alpha Olomi",
  "main": "dist/index.js",
  "module": "dist/node-fetch-demo.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prettier": "tsdx lint --fix",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "node-fetch": "3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "@types/node-fetch": "3",
    "husky": "^7.0.4",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/node-fetch-demo.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/node-fetch-demo.esm.js",
      "limit": "10 KB"
    }
  ]
}
